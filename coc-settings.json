{
  "coc.preferences.formatOnSaveFiletypes": ["css", "js", "jsx", "ts", "tsx", "markdown", "typescriptreact", "scss", "rust"],
  "languageserver": {
   "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
